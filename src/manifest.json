{

  "manifest_version": 2,
  "name": "Hololive-Livestream-Assistant",
  "version": "1.0",

  "description": "",

  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": [
    "webRequest",
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/watch?v=*"],
      "js": ["borderify.js"]
    }
  ]

}